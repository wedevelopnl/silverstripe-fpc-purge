{
    "name": "wedevelopnl/silverstripe-fpc-purge",
    "description": "FPC purge module",
    "keywords": [
        "silverstripe",
        "fpc",
        "cache",
        "purge"
    ],
    "authors": [{
        "name": "WeDevelop",
        "email": "development@wedevelop.nl"
    }],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "silverstripe/cms": "^4.6.0@stable"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "extra": {
        "installer-name": "silverstripe-fpc-purge",
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\FPCPurge\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "silverstripe/vendor-plugin": false
        }
    }
}
